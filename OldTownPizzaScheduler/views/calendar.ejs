<style>
textarea{
    display: block;
    margin-left: auto;
    margin-right: auto;
    font-size: 13px;
}

.sidebar {
  height: 100%;
  width: 0;
  position: fixed;
  z-index: 1;
  top: 0;
  left: 0;
  background-color: #c5c2c2;
  overflow-x: hidden;
  transition: 0.5s;
  padding-top: 60px;
}

.sidebar a {
  padding: 8px 8px 8px 32px;
  text-decoration: none;
  font-size: 25px;
  color: black;
  display: block;
  transition: 0.3s;
}

.sidebar a:hover {
  color: #f1f1f1;
}

.sidebar .closebtn {
  position: absolute;
  top: 0;
  right: 25px;
  font-size: 36px;
  margin-left: 50px;
}

@media screen and (min-width: 850px) {
  .wrapper {
    grid-template-columns: 200px 5fr;
    grid-template-rows: 1fr;
    grid-template-areas: "sidebar content";
  }
}

/* MAIN */

.calendar__week,
.calendar__header {
  display: grid;
  grid-template-columns: repeat(7, 1fr);  
}
.calendar__week {
  grid-auto-rows: 100px;
  text-align: right;
}

.calendar__header {
  grid-auto-rows: 50px;
  align-items: center;
  text-align: center;
}

.m{
  display: grid;
  grid-template-columns: repeat(5, 1fr);  
  grid-columns: 3;
  grid-auto-rows: 100px;
  text-align: right;
}
.calendar__day{
  padding: 16px;
}

.calendar {
  background-color: white;
  border: 1px solid #e1e1e1;
}

.calendar__header > div {
  text-transform: uppercase;
  font-size: 0.8em;
  font-weight: bold;
}

.calendar__day {
  border-right: 1px solid #e1e1e1;
  border-top: 1px solid #e1e1e1;
  text-decoration:none;
}

.calendar__day:last-child {
  border-right: 0;
    text-decoration:none;
}

.toggle{
  display: grid;
  grid-template-columns: 1fr 1fr;
  text-align: center;
  font-size: 0.9em;
}

a.hover{
  text-decoration:none;
}

a{
  color:black;
}
